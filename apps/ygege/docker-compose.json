{
  "schemaVersion": 2,
  "services": [
    {
      "name": "ygege",
      "image": "uwucode/ygege:0.8.0",
      "environment": [
        {
          "key": "YGG_USERNAME",
          "value": "${YGG_USERNAME}"
        },
        {
          "key": "YGG_PASSWORD",
          "value": "${YGG_PASSWORD}"
        },
        {
          "key": "LOG_LEVEL",
          "value": "debug"
        }
      ],
      "internalPort": 8715,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/sessions",
          "containerPath": "${APP_DATA_DIR}/sessions",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ],
      "healthCheck": {
        "test": "curl --fail http://localhost:$${BIND_PORT:-8715}/health || exit 1",
        "interval": "1m30s",
        "timeout": "20s",
        "retries": 3,
        "startPeriod": "10s"
      },
      "isMain": true,
      "extraLabels": {
        "runtipi.managed": true
      }
    }
  ]
}